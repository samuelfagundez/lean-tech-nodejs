{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "target": "es5",
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "sourceMap": true,
   }
}